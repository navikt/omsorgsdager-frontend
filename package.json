{
    "name": "omsorgsdager-frontend",
    "version": "2.0.3",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "start": "node ./server.js",
        "build": "webpack --config webpack/webpack-config.production.js",
        "buildNewVersion": "npm version patch --no-git-tag-version && webpack --config webpack/webpack-config.production.js",
        "buildDev": "webpack --config webpack/webpack-config.production.js && yarn calculateHashOnLatestBuild && yarn dev",
        "calculateHashOnLatestBuild": "node calculateHashFromBuild.js app.js && node calculateHashFromBuild.js styles.css",
        "dev": "node webpack/webpack-config.development.js",
        "version": "node ./version.js",
        "lint": "eslint src/**/*.{ts,tsx,js,jsx}",
        "lint:css": "stylelint src/**/*.{less,css}",
        "checkPackageUpgrade": "yarn test && yarn lint && yarn lint:css && yarn buildDev"
    },
    "keywords": [],
    "author": "NAV IT",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/node": "^7.14.5",
        "@babel/preset-env": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.14.5",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.5",
        "@types/jest": "^26.0.23",
        "@types/jest-axe": "^3.5.1",
        "@types/testing-library__jest-dom": "^5.14.0",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^5.2.6",
        "css-minimizer-webpack-plugin": "^3.0.1",
        "eslint": "^7.28.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.1.2",
        "file-loader": "^6.1.1",
        "html-webpack-plugin": "^5.3.1",
        "jest": "^26.6.3",
        "jest-axe": "^4.1.0",
        "jest-environment-jsdom-fourteen": "^1.0.1",
        "jest-extended": "^0.11.5",
        "jest-transform-stub": "^2.0.0",
        "less": "^4.1.1",
        "less-loader": "^10.0.0",
        "mini-css-extract-plugin": "^1.6.0",
        "stylelint": "^13.7.2",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-order": "^4.1.0",
        "terser-webpack-plugin": "^5.1.3",
        "ts-jest": "^26.4.4",
        "typescript": "^4.3.2",
        "webpack": "^5.39.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@navikt/fnrvalidator": "^1.1.3",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.7",
        "classnames": "^2.2.6",
        "cors": "^2.8.5",
        "dayjs": "^1.10.5",
        "express": "^4.17.1",
        "lodash": "^4.17.21",
        "lodash.throttle": "^4.1.1",
        "nav-datovelger": "^11.3.0",
        "nav-frontend-alertstriper": "^4.0.1",
        "nav-frontend-alertstriper-style": "^3.0.1",
        "nav-frontend-core": "^6.0.1",
        "nav-frontend-ikoner-assets": "^3.0.1",
        "nav-frontend-js-utils": "^1.0.18",
        "nav-frontend-knapper": "^3.0.1",
        "nav-frontend-knapper-style": "^2.0.1",
        "nav-frontend-lenker": "^2.0.1",
        "nav-frontend-lenker-style": "^2.0.1",
        "nav-frontend-paneler-style": "^2.0.1",
        "nav-frontend-skjema": "^4.0.1",
        "nav-frontend-skjema-style": "^3.0.1",
        "nav-frontend-spinner": "^3.0.1",
        "nav-frontend-spinner-style": "^1.0.2",
        "nav-frontend-typografi": "^4.0.1",
        "nav-frontend-typografi-style": "^2.0.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-day-picker": "^7.4.10",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.8.7"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/types.ts",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(ts|tsx|js|jsx)?$": "ts-jest",
            "^.+.(css|less)$": "jest-transform-stub"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [
            "<rootDir>/src"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ]
    }
}
