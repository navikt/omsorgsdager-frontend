        "dev": "node webpack/webpack-config.development.js",
        "version": "node ./version.js",
        "lint": "eslint src/**/*.{ts,tsx,js,jsx}",
        "lint:css": "stylelint src/**/*.{less,css}",
        "checkPackageUpgrade": "yarn test && yarn lint && yarn lint:css && yarn build"
    },
    "keywords": [],
    "author": "NAV IT",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.9",
